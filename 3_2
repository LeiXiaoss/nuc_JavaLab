import java.util.*;

public class Frequency {
    public int getFrequency(String[] article, int n, String word) {
	        // write code here        
				int q=0;
	        for(int i = 0;i < article.length;i++){
				if(word.equals(article[i]))
	             {
	               q++ ;
	            }
	        }
			return q;
	    }
}
