public class NextElement 
{
	public int[] findNext(int[] A)
	{
		int n = A.length;
		for(int i = 0; i < n; i++)
		{
			for(int j = i+1; j<n; j++)
			{
				if(A[i] < A[j])
				{
					A[i] = A[j];
					break;
				}
				if(j==n-1)
					A[i] = -1;
			}

		}
		A[n-1] = -1;
		return A; 
	}
	public static void main(String[] args)
	{
		NextElement a = new NextElement();
		int[] A = {5, 3, 12, 54, 1,23};
		A = a.findNext(A);
		for(int i = 0; i < A.length; i++)
			System.out.println(A[i]);
	}
}
